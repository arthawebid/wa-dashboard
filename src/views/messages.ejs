<h3>Messages</h3>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Session</th>
      <th>Sender</th>
      <th>Message</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% messages.forEach(msg => { %>
      <tr>
        <td><%= msg.session %></td>
        <td><%= msg.sender %></td>
        <td><%= msg.message %></td>
        <td><%= msg.created_at %></td>
      </tr>
    <% }) %>
  </tbody>
</table>
